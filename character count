// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
    // Write C++ code here
    string s;
    char ch;
    cin>>s>>ch;
    int i,c=0;
    for(i=0;i<s.length();i++){
        if(s[i]==ch)
        c++;
    }
    cout<<c;

    return 0;
}
output
hello l
2
