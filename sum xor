find the sum of elements at odd indices and xor of elements at even . find the difference of sum and xori
#include <iostream>
using namespace std;
int main() {
    // Write C++ code here
int n;
cin>>n;
int a[n],i,sum=0,xori=0;
for(i=0;i<n;i++){
    cin>>a[i];
    if(i%2==1)
    sum+=a[i];
    else
    xori=xori^a[i];
}
cout<<sum-xori;
    return 0;
}
